@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>If this not working anymore, try restart your discord.</p>
    <form action="/Home/UpdateStatus" method="post" id="form">
        <div class="form-group">
            <label for="State">State</label>
            <input type="text" class="form-control" id="State" name="State" />
        </div>
        <div class="form-group">
            <label for="Details">Details </label>
            <input type="text" class="form-control" id="Details" name="Details" />
        </div>
        <div class="form-group">
            <label for="timeStart">timeStart </label>
            <div class="input-group">
                <div class="input-group-prepend">
                    <button onclick="$('#timeStart').val(Date.now())" class="btn btn-outline-secondary" type="button">
                        Use current time
                    </button>
                </div>
                <input type="text" class="form-control" id="timeStart" name="timeStart" />
            </div>
        </div>
        <div class="form-group">
            <label for="timeStart">timeEnd </label>
            <div class="input-group">
                <div class="input-group-prepend">
                    <button onclick="$('#timeEnd').val(Date.now())" class="btn btn-outline-secondary" type="button">
                        Use current time
                    </button>
                </div>
                <input type="text" class="form-control" id="timeEnd" name="timeEnd" />
            </div>
        </div>
        <div class="form-group">
            <label for="LargeImage">LargeImage  </label>
            <input type="text" class="form-control" id="LargeImage" name="LargeImage" />
        </div>
        <div class="form-group">
            <label for="LargeText">LargeText </label>
            <input type="text" class="form-control" id="LargeText" name="LargeText" />
        </div>
        <div class="form-group">
            <label for=" SmallImage"> SmallImage </label>
            <input type="text" class="form-control" id=" SmallImage" name="SmallImage" />
        </div>
        <div class="form-group">
            <label for="SmallText">SmallText  </label>
            <input type="text" class="form-control" id="SmallText" name="SmallText" />
        </div>
        <div class="form-group form-check">
            <div class="custom-control custom-checkbox">
                <input type="checkbox" class="custom-control-input" id="EnableParty" name="EnableParty" onclick="EnablePartyClicked()" />
                <label class="custom-control-label" for="EnableParty">EnableParty</label>
            </div>
        </div>
        <div class="form-group">
            <label for="CurrentSize">Party Current Size default:1</label>
            <input type="text" class="form-control" id="CurrentSize" name="CurrentSize" disabled />
        </div>
        <div class="form-group">
            <label for="MaxSize">MaxSize default:1</label>
            <input type="text" class="form-control" id="MaxSize" name="MaxSize" disabled />
        </div>
        <input type="button" name="submit" value="submit" onclick="onFormSubmit()" class="btn btn-success" />
    </form>
</div>
<div aria-live="polite" aria-atomic="true" style="position: relative; min-height: 200px;">
    <div class="toast" style="position: absolute; top: 0; right: 0;" id="successToast">
        <div class="toast-header">
            <img src="data:image/png;base64," class="rounded mr-2" alt="...">
            <strong class="mr-auto">Success</strong>
            <small>now</small>
            <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="toast-body">
            Your request has been sent successfully.
        </div>
    </div>
</div>
@section scripts    {
    <script>
        function onFormSubmit() {
            $.ajax({
                url: "/Home/UpdateStatus",
                type: 'post',
                data: $('#form').serialize(),
                success: function () {
                    console.log("success")
                    $('#successToast').toast('show')
                }
            });
        }
        function EnablePartyClicked() {
            if ($("#EnableParty").prop("checked")) {
                $("#MaxSize").prop("disabled", false);
                $("#CurrentSize").prop("disabled", false);
            } else {
                $("#MaxSize").prop("disabled", true);
                $("#CurrentSize").prop("disabled", true);
            }
        }
    </script>
}